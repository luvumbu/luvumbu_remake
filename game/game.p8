version 38
__lua__
function _init()
    -- position initiale du personnage
    x = 64
    y = 64
    
    -- vitesse de dれたplacement
    speed = 2
end

function _update()
    -- dれたplacement vers la gauche
    if btn(0) then
        x = x - speed
    end
    
    -- dれたplacement vers la droite
    if btn(1) then
        x = x + speed
    end
    
    -- limites de l'れたcran
    if x < 0 then x = 0 end
    if x > 120 then x = 120 end
end

function _draw()
    -- effacer l'れたcran
    cls(0)
    
    -- dessiner le personnage (carrれた de 8x8 pixels)
    rectfill(x, y, x+7, y+7, 12)
    
    -- afficher les instructions
    print("flれそches gauche/droite", 10, 10, 7)
end
